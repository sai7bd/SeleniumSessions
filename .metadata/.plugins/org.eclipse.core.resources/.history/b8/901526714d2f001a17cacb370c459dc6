import java.util.ArrayList;
import java.util.Iterator;

public class Collection {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Employee emp1 = new Employee("Saif",33,"IT");
		Employee emp2 = new Employee("Jhumur",33,"Walmart");
		Employee emp3 = new Employee("Annhara",33,"School");
		
		ArrayList<Employee> emp = new ArrayList<Employee>();
		emp.add(emp1);
		emp.add(emp2);
		emp.add(emp3);
		
		Iterator<Employee> it = emp.iterator();
		System.out.println(" Name :" +  " Age :" + " Dept: ");
		while(it.hasNext()) {
			Employee empr = it.next();
			System.out.println(empr.Name + " " + empr.Age + " " + empr.Dept);
		}
		
		
		
		
	}

}
